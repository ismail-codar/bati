{
  "name": "@batijs/tailwindcss",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "bati-compile-boilerplate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@batijs/compile": "workspace:*",
    "@types/node": "^18.19.68",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.22",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "vike": "^0.4.210",
    "vite": "^6.0.5"
  },
  "dependencies": {
    "@batijs/core": "workspace:*"
  },
  "files": [
    "dist/"
  ],
  "bati": {
    "if": {
      "flag": "tailwindcss"
    }
  },
  "exports": {
    "./postcss.config": {
      "types": "./dist/types/postcss.config.d.ts"
    },
    "./tailwind.config": {
      "types": "./dist/types/tailwind.config.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "postcss.config": [
        "./dist/types/postcss.config.d.ts"
      ],
      "tailwind.config": [
        "./dist/types/tailwind.config.d.ts"
      ]
    }
  }
}