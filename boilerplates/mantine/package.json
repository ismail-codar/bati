{
  "name": "@batijs/mantine",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "bati-compile-boilerplate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@batijs/compile": "workspace:*",
    "@mantine/core": "^7.15.3",
    "@mantine/hooks": "^7.15.3",
    "@types/node": "^18.19.70",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vike-react": "^0.5.11",
    "vite": "^6.0.7",
    "vite-plugin-compiled-react": "^1.1.3"
  },
  "dependencies": {
    "@batijs/core": "workspace:*"
  },
  "files": [
    "dist/"
  ],
  "bati": {
    "enforce": "post",
    "if": {
      "flag": {
        "$all": [
          "mantine",
          "react"
        ]
      }
    }
  },
  "exports": {
    "./postcss.config.": {
      "types": "./dist/types/postcss.config.d.cts"
    },
    "./components/Link": {
      "types": "./dist/types/components/Link.d.ts"
    },
    "./layouts/theme": {
      "types": "./dist/types/layouts/theme.d.ts"
    },
    "./layouts/LayoutDefault": {
      "types": "./dist/types/layouts/LayoutDefault.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "postcss.config.": [
        "./dist/types/postcss.config.d.cts"
      ],
      "components/Link": [
        "./dist/types/components/Link.d.ts"
      ],
      "layouts/theme": [
        "./dist/types/layouts/theme.d.ts"
      ],
      "layouts/LayoutDefault": [
        "./dist/types/layouts/LayoutDefault.d.ts"
      ]
    }
  }
}